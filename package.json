{
  "name": "ZynetMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "npm run build && jest",
    "tsc": "tsc",
    "clean": "rimraf artifacts",
    "build": "npm run clean && npm run tsc --",
    "lint": "tslint src/**/*.ts",
    "watch": "npm run build -- -w",
    "ios": "npm run build && concurrently -r 'npm run watch' 'react-native run-ios'",
    "android": "npm run build && concurrently -r 'npm run watch' 'react-native run-android'"
  },
  "dependencies": {
    "keymirror": "^0.1.1",
    "moment": "^2.22.2",
    "native-base": "^2.4.3",
    "react": "16.4.1",
    "react-native": "0.56.1",
    "react-native-modal": "^6.1.0",
    "react-native-svg": "^6.4.1",
    "react-native-svg-charts": "^5.2.0",
    "react-native-vector-icons": "^4.6.0",
    "react-navigation": "^2.18.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "tslib": "^1.9.2"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/node": "^10.3.5",
    "@types/react": "^16.3.18",
    "@types/react-native": "^0.55.21",
    "babel-jest": "22.4.1",
    "babel-preset-react-native": "5.0.2",
    "concurrently": "^3.5.1",
    "jest": "22.4.2",
    "react-test-renderer": "^16.3.0-alpha.1",
    "redux-devtools": "^3.4.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "typescript": "3.1.3"
  },
  "jest": {
    "preset": "react-native",
    "testRegex": "artifacts/.+\\.(test|spec).js$",
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text-summary",
      "html"
    ],
    "collectCoverageFrom": [
      "artifacts/**/*.js",
      "!artifacts/**/*.spec.js",
      "!artifacts/**/*.index.js"
    ]
  }
}
